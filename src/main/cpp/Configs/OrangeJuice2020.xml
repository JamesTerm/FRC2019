<?xml version="1.0" encoding="UTF-8"?>
<!-- Root of the Config -->
<Robot>
    <!-- Version of the current config to be printed on the console -->
    <Version version="1" />
    <!-- Comment on this particular config to be printed on the console -->
    <Comment comment="A" />
    <!-- Sets whether or not to initialize the NavX -->
    <UseNavX value="true" />
    <!-- Sets whether or not to initialize the camera to send back to the driver station -->
    <!-- Contains assignments to ports and robot dimensions -->
    <RobotConfig>
        <!-- Region for defining VictorSP Items -->
        <VictorSP>
           <shooter0 channel="8" reversed=""/>
           <shooter1 channel="9" reversed=""/>
        </VictorSP>
        <!-- Region for VictorSPX CAN Items -->
        <VictorSPX>
            <right_0 channel="0" reversed="false"/>
            <right_1 channel="1" reversed="false"/>
            <right_2 channel="2" reversed="false"/>
            <left_0 channel="3" reversed="false"/>
            <left_1 channel="4" reversed="false"/>
            <left_2 channel="5" reversed="false"/>
        </VictorSPX>
        <!-- Region for TalonSRX CAN Items -->

        <TalonSRX>
        </TalonSRX>
        <!-- Region for Potentiometers -->
        <Potentiometer>
        </Potentiometer>
        <!-- Region for Encoders -->
        <Encoder>
        </Encoder>
        <!-- Region for DoubleSolenoids -->
        <DoubleSolenoid>

		</DoubleSolenoid>
        <!-- Region for DigitalInputs -->
        <DigitalInput>
            <!--limSwitch channel="4"/-->
            
        </DigitalInput>
    </RobotConfig>

    <!-- Root of all control definitions -->
    <Controls>
        <!-- Driver Controller, slot should always be 0 -->
        <Driver slot="0">
            <AxisControls>
                <leftDrive axis="1" deadZone="0.08" reversed="true" powerMultiplier="0.8" bindings="left_2" useOverdrive="true"/>
                <rightDrive axis="5" deadZone="0.08" reversed="false" powerMultiplier="0.8" bindings="right_0, right_1, right_2" useOverdrive="true"/>
            </AxisControls>
          <ButtonControls>
                <overdrive button="5" powerMultiplier="1.0" isOverdrive="true" />
          </ButtonControls>
          <ToggleButtonControls>
                <endgame_control button="3" powerMultiplier="1.0" bindings="endgame"/>
          </ToggleButtonControls-->
        </Driver>
		<Operator slot="1">
			<AxisControls>
                <outtake_control axis="2" deadZone="0.08" reversed="false" powerMultiplier="1.0" bindings="intake"/>
				<elevator_control axis="1" deadZone="0.08" reversed="false" powerMultiplier="0.75" bindings="elevator_0, elevator_1, elevator_2, elevator_3" />
                <intake_control axis="3" deadZone="0.08" reversed="true" powerMultiplier="1.0" bindings="intake"/>
			</AxisControls>
			<ButtonControls>
        <intakeButton button="4" powerMultiplier="0.8" isRamp="true" isAmpLimited="false" actOnRelease="true"/>
			</ButtonControls>
            <GoalButtonControls>
                <elePreset_0 button="1" goal="ElevatorControl" params="0"/>
                <elePreset_1 button="3" goal="ElevatorControl" params=".25"/>
                <elePreset_2 button="4" goal="ElevatorControl" params=".70"/>
                <releEle button="8" goal="RelativeElevatorControl" params="-.07"/>
            </GoalButtonControls>
			<ToggleButtonControls>
                <hatch_push_control button="5" powerMultiplier="1.0" bindings="hatch_push"/>
				<wrist_control button="10" powerMultiplier="1.0" bindings="wrist"/>
                
			</ToggleButtonControls>
		</Operator>
    </Controls>
</Robot>